import java.util.Scanner;

public class Reverse {
    
    public static void main(String[] args) {
        int n, temp, sum = 0, r;
        Scanner input = new Scanner(System.in);
        System.out.print("Enter a number: ");

        n = input.nextInt();
        temp = n;
        while(temp!= 0)
        {
            r = temp%10;
            sum = sum*10+r;
            temp = temp/10;
        }
        System.out.print(sum);
    }
}
