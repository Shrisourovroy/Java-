import java.util.Scanner;
public class SumOfDigit {
    public static void main()
    {
        int n, temp, r, sum = 0;
        Scanner input = new Scanner(System.in);
        System.out.print("Enter number: ");
        n = input.nextInt();
        temp = n;

        while(temp>0)
        {
            r = temp%10;
            sum = sum + r;
            temp = temp / 10;
        }

        System.out.print(sum);
    }
}
