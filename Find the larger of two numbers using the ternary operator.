import java.util.Scanner;

public class GreaterT {
    public static void main(String[] args) {
        int a,b,c;
        Scanner input = new Scanner(System.in);
        System.out.print("Enter two number: ");
        a = input.nextInt();
        b = input.nextInt();
        c = (a>b) ? a:b ;
        System.out.print("Greater = "+c);       
    }
}
