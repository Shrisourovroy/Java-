public class ForDm {
    public static void main(String[]args){
        System.out.print("From for loop: ");
        for(int i=1; i<=100; i++)
        {
            System.out.print(" "+i);
        }
        System.out.print("\n\nFrom while loop: ");
        int j = 0;
        while(j<=100){
            System.out.print(" "+j);
            j++;
        }
        System.out.print("\n\nFrom do__while loop: ");
        
        int k = 1;
        do{
            System.out.print(" "+k);
          k++;
        }while(k<=100);
          
    }
}
